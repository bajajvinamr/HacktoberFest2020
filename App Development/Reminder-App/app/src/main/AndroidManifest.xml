<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jainvidushi.remapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SetTaskActivity" />
        <activity android:name=".UpcomingTaskActivity" />
        <activity android:name=".RunTaskActivity" />
        <activity android:name=".ViewTaskActivity" />
        <activity android:name=".Menu_NavActivity" />
        <activity android:name=".EditTaskActivity" />
        <activity android:name=".DeleteActivity" />
        <activity android:name=".StatusAssigningActivity" />
        <activity android:name=".SubViewActivity" />
        <activity android:name=".SubEditActivity" />
        <activity android:name=".Sub_Delete_Activity" />
        <activity android:name=".DeveloperActivity"></activity>
    </application>

</manifest>